<h1 class="text-center">Welcome to contact page</h1>
<table class="table table-striped mt-3">
    <thead>
      <tr>
        <th scope="col">No</th>
        <th scope="col">Nama</th>
        <th scope="col">Email</th>
        <th scope="col">No HP</th>
        <th scope="col">Details</th>
      </tr>
    </thead>
    <tbody>
      <% if (contacts.length===0) { %>
         <tr>
          <td colspan="5">
            <div class="alert alert-danger" role="alert">
              <b>Contact hasn't been filled</b>
            </div>
          </td>
         </tr>
      <% } %>
      <% contacts.forEach((contact,i) => { i++%>
        <tr>
          <th scope="row"><%= i++ %></th>
          <td><%=contact.nama %></td>
          <td><%=contact.email %></td>
          <td><%=contact.NoHP %></td>
          <td><a href="/contact/<%=contact.nama  %>" class="badge badge-success"><i class="bi bi-info-circle"> </i>detail</a></td>
        </tr>
      <% }) %>
      <a href="/contact/add">
      <button type="button" class="btn btn-primary">Tambahkan Data</button></a>
    </tbody>
  </table>