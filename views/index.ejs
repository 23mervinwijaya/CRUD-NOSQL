    <h1>Welcome to Homepage</h1>
    <h2>Daftar Mahasiswa</h2>
    <% if (mahasiswa.length <1) { %>
        <p>Data mahasiswa masih kosong</p>
    <% }else{ %>

    <% mahasiswa.forEach(mhs => { %>
        <ul>
            <li>nama = <%=mhs.nama  %></li>
            <li>kelas = <%=mhs.kelas  %></li>
            <li>noTelp = <%=mhs.noTelp  %></li>
        </ul> 
    <% })} %>